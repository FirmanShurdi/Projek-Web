<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Edit alat tangkap</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
   <div class="d-flex justify-content-center mt-4 mb-4">
        <div class="container py-4 bg-light p-4 rounded">
            <h1>Edit alat tangkap</h1>
            <form action="/alat_tangkap/update/<%= alat_tangkap.id_alat_tangkap %>" method="POST">
                <input type="hidden" name="id_alat_tangkap" value="<%= alat_tangkap.id_alat_tangkap %>">
                
                <!-- Input untuk nama produk -->
                <div class="mb-3">
                    <label for="nama_alat_tangkap">Nama alat tangkap</label>
                    <input type="text" name="nama_alat_tangkap" id="nama_alat_tangkap" class="form-control" value="<%= alat_tangkap.nama_alat_tangkap %>">
                </div>
        
                <!-- Tombol untuk menyimpan perubahan -->
                <div class="mb-3">
                    <button type="submit" class="btn btn-sm btn-primary">Simpan</button>
                </div>
            </form>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>